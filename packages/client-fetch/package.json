{
  "name": "@ts-sdk-gen/client-fetch",
  "version": "1.0.2",
  "description": "ðŸš€ Fetch API client for `@abdulrahmanmousa/openapi-ts` codegen.",
  "license": "MIT",
  "author": "Abdulrahman Mousa",
  "private": false,
  "keywords": [
    "client",
    "fetch",
    "http",
    "javascript",
    "openapi",
    "react",
    "rest",
    "svelte",
    "swagger",
    "typescript",
    "vue"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE.md",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup && pnpm check-exports",
    "check-exports": "attw --pack .",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest watch --update",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "vitest --typecheck --watch=false"
  },
  "dependencies": {
    "qs": "^6.14.0"
  },
  "devDependencies": {
    "@types/qs": "^6.9.18"
  },
  "directories": {
    "test": "test"
  }
}
