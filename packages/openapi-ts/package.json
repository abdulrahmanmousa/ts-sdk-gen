{
  "name": "@ts-sdk-gen/openapi-ts",
  "version": "0.0.6",
  "description": "ðŸš€ The OpenAPI to TypeScript codegen. Generate clients, SDKs, validators, and more. Forked from @hey-api/openapi-ts.",
  "homepage": "https://github.com/abdulrahmanmousa/ts-sdk-gen",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abdulrahmanmousa/ts-sdk-gen.git"
  },
  "bugs": {
    "url": "https://github.com/abdulrahmanmousa/ts-sdk-gen/issues"
  },
  "license": "MIT",
  "author": "Abdulrahman Mousa",
  "keywords": [
    "angular",
    "axios",
    "codegen",
    "fetch",
    "generator",
    "http",
    "javascript",
    "json",
    "node",
    "openapi",
    "rest",
    "swagger",
    "typescript",
    "xhr",
    "yaml"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "openapi-ts": "bin/index.cjs"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE.md"
  ],
  "scripts": {
    "build": "tsup && pnpm check-exports",
    "check-exports": "attw --pack .",
    "dev": "tsup --watch",
    "handlebars": "node src/legacy/handlebars/handlebars.cjs",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --config vitest.config.unit.ts --coverage",
    "test:e2e": "vitest run --config vitest.config.e2e.ts",
    "test:sample": "node test/sample.cjs",
    "test:update": "vitest watch --config vitest.config.unit.ts --update",
    "test:watch": "vitest watch --config vitest.config.unit.ts",
    "test": "vitest run --config vitest.config.unit.ts",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "11.7.2",
    "c12": "2.0.1",
    "commander": "12.1.0",
    "handlebars": "4.7.8",
    "pluralize": "8.0.0",
    "prompts": "2.4.2"
  },
  "peerDependencies": {
    "typescript": "^5.x"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.1",
    "@angular/animations": "19.0.0",
    "@angular/cli": "19.0.1",
    "@angular/common": "19.0.0",
    "@angular/compiler": "19.0.0",
    "@angular/compiler-cli": "19.0.0",
    "@angular/core": "19.0.0",
    "@angular/forms": "19.0.0",
    "@angular/platform-browser": "19.0.0",
    "@angular/platform-browser-dynamic": "19.0.0",
    "@angular/router": "19.0.0",
    "@tanstack/angular-query-experimental": "5.61.3",
    "@tanstack/react-query": "5.61.3",
    "@tanstack/solid-query": "5.51.21",
    "@tanstack/svelte-query": "5.61.3",
    "@tanstack/vue-query": "5.61.3",
    "@ts-sdk-gen/client-axios": "workspace:*",
    "@ts-sdk-gen/client-fetch": "workspace:*",
    "@types/cross-spawn": "6.0.6",
    "@types/express": "4.17.21",
    "@types/prompts": "2.4.9",
    "axios": "1.7.7",
    "cross-spawn": "7.0.5",
    "eslint": "9.6.0",
    "express": "4.21.0",
    "fastify": "5.1.0",
    "glob": "10.4.3",
    "node-fetch": "3.3.2",
    "prettier": "3.3.2",
    "puppeteer": "22.12.1",
    "rxjs": "7.8.1",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "typescript": "5.5.3",
    "zod": "3.23.8"
  },
  "directories": {
    "test": "test"
  }
}
