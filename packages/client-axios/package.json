{
  "name": "@ts-sdk-gen/client-axios",
  "version": "1.0.0",
  "description": "ðŸš€ Axios client for `@ts-sdk-gen/openapi-ts` codegen.",
  "license": "MIT",
  "author": {
    "name": "Abdulrahman Mousa"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "axios",
    "client",
    "http",
    "javascript",
    "openapi",
    "react",
    "rest",
    "svelte",
    "swagger",
    "typescript",
    "vue"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "LICENSE.md",
    "src"
  ],
  "scripts": {
    "build": "tsup && pnpm check-exports",
    "check-exports": "attw --pack .",
    "dev": "tsup --watch",
    "prepublishOnly": "pnpm build",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest watch --update",
    "test:watch": "vitest watch",
    "test": "vitest run",
    "typecheck": "vitest --typecheck --watch=false"
  },
  "peerDependencies": {
    "axios": ">= 1.0.0 < 2"
  },
  "devDependencies": {
    "axios": "1.7.7"
  }
}
